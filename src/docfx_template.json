{
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "_version_/api",
        "dest": "api"
      },
      {
        "files": ["**/*.{md,yml}"],
        "src": "_version_/docs",
        "dest": "docs"
      },
      {
        "files": ["toc.yml", "index.md"],
        "src": "_version_"
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetRDF Documentation"
    },
    "dest": "../build/_version_",
    "template": "default"
  }
}
{
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "$version$/api",
        "dest": "api"
      },
      {
        "files": ["**/*.{md,yml}"],
        "src": "$version$/docs",
        "dest": "docs"
      },
      {
        "files": ["toc.yml", "index.md"],
        "src": "$version$"
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetRDF Documentation"
    },
    "dest": "../build/$version$",
    "template": "default"
  }
}
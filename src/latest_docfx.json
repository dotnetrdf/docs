{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**"],
          "src": "../../dotNetRDF/Libraries"
        }
      ],
      "dest": "latest/api",
      "properties": {
          "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "latest/api",
        "dest": "api"
      },
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["api/**/*.{md,yml}"],
        "src": "latest",
      },
      {
        "files": ["toc.yml", "index.md"],
        "src": "latest"
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "src": "common"
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetRDF Documentation",
      "_appLogoPath": "images/rdf_flyer.svg"
    },
    "dest": "../build/latest",
    "template": ["default", "templates/latest"]
  }
}
{
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "stable/api",
        "dest": "api"
      },
      {
        "files": ["**/*.{md,yml}"],
        "src": "stable/docs",
        "dest": "docs"
      },
      {
        "files": ["toc.yml", "index.md"],
        "src": "stable"
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetRDF Documentation"
    },
    "dest": "../build/stable",
    "template": "default"
  }
}